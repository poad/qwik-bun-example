{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "test.e2e": "playwright test",
    "test.unit": "vitest components",
    "test.unit.ui": "vitest --ui components",
    "qwik": "qwik",
    "prettier-check": "prettier --check src",
    "prettier-format": "prettier --write src",
    "lint-fix": "bun run prettier-format && bun run lint --fix"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.10.2",
    "@builder.io/qwik": "^1.7.3",
    "@builder.io/qwik-city": "^1.7.3",
    "@playwright/test": "^1.45.3",
    "@storybook/addon-essentials": "^8.2.6",
    "@storybook/addon-links": "^8.2.6",
    "@storybook/blocks": "^8.2.6",
    "@storybook/builder-vite": "^8.2.6",
    "@storybook/html": "^8.2.6",
    "@storybook/html-vite": "^8.2.6",
    "@types/node": "^22.0.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/ui": "^2.0.5",
    "eslint": "^9.8.0",
    "eslint-plugin-qwik": "^1.7.3",
    "prettier": "^3.3.3",
    "storybook": "^8.2.6",
    "storybook-framework-qwik": "^0.4.0",
    "typescript": "^5.5.4",
    "undici": "^6.19.5",
    "vite": "^5.3.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.5"
  },
  "packageManager": "pnpm@9.0.6+sha512.f6d863130973207cb7a336d6b439a242a26ac8068077df530d6a86069419853dc1ffe64029ec594a9c505a3a410d19643c870aba6776330f5cfddcf10a9c1617"
}
