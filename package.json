{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint src/**/*.ts*",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "test.e2e": "playwright test",
    "test.unit": "vitest components",
    "test.unit.ui": "vitest --ui components",
    "qwik": "qwik",
    "prettier-check": "prettier --check src",
    "prettier-format": "prettier --write src",
    "lint-fix": "bun run prettier-format && bun run lint --fix"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.1",
    "@builder.io/qwik": "^1.3.1",
    "@builder.io/qwik-city": "^1.3.1",
    "@playwright/test": "^1.40.1",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/blocks": "^7.6.6",
    "@storybook/builder-vite": "^7.6.6",
    "@storybook/html": "^7.6.6",
    "@storybook/html-vite": "^7.6.6",
    "@types/eslint": "^8.56.0",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-qwik": "^1.3.1",
    "prettier": "^3.1.1",
    "storybook": "^7.6.6",
    "storybook-framework-qwik": "^0.2.5",
    "typescript": "^5.3.3",
    "undici": "^6.2.1",
    "vite": "^5.0.10",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.0"
  }
}
